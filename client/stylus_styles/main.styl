#terminal
  width 100%

.topsoilInputBox
 margin 10px

.inputText
  height 100%

.regexEditor:hover
  background-color pink

.regexInput
  width 200px

.activeSuggestion
  background-color #E7E7E7

.suggestionName
  padding-left 8px

.suggestionDescription
  color gray
  padding-left 8px

.suggestionArguments
  border-radius 25px
  padding-left 3px
  padding-right 3px
  background #FAD587
  margin-left 10px

.f-icon
  width 25px
  vertical-align: middle

.flex-container
  padding: 0
  margin: 0
  list-style: none

  display: -webkit-box
  display: -moz-box
  display: -ms-flexbox
  display: -webkit-flex
  display: flex

  -webkit-flex-flow: row wrap


.flex-item
  opacity: 1

  width: 200px
  height: 150px


  line-height: 150px
  color: white
  font-weight: bold
  font-size: 2em
  text-align: center
  transition: opacity 1s

.flex-item:hover
  opacity: 0.5
  transition: opacity 1s

.subviews
  border-style: solid
  border-width: 2px
  height 100%


.icon
  padding-left 5px
  width 25px

.parentRow
  padding 15px

.magicinputs
  margin-right 1px
  font-size 15px
.magicinputs:focus
  outline none
#command
  border-radius 5px 5px 5px 5px
  background #ECD9DA
  border 10px solid #ECD9DA
.magicinputs.args
  border-radius 20px 20px 20px 20px
  border 10px solid #ECD9DA
  background #ECD9DA
